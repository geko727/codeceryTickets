<h5>Register</h5>

<%= form_for @user do |f| %>
        <%= f.label :name %>
        <%= f.text_field :name %>
        </br>
        <%= f.label :last_name %>
        <%= f.text_field :last_name %>
        </br><%= f.label :email %>
        <%= f.text_field :email %>
        </br>
        <%= f.label :company %>
        <%= f.text_field :company %>
        </br>
        <%= f.label :password %>
        <%= f.password_field :password %>
        </br>
        <%= f.submit "Register", class: 'btn btn-primary' %>
<% end %>